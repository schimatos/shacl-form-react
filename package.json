{
  "name": "shacl-form-react",
  "version": "0.0.0-development",
  "description": "A HTML5 form generator for any SHACL constraint compliant to the W3C standard.",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test": "jest",
    "lint": "eslint lib/** __tests__/* --ext .ts --ext .tsx",
    "lint:fix": "eslint lib/** __tests__/* --ext .ts --ext .tsx --fix",
    "build": "tsc && cp lib/styles.css dist/styles.css",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "typedoc": "typedoc lib/index.ts",
    "webpack": "webpack --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schimatos/shacl-form-react"
  },
  "keywords": [
    "SHACL",
    "form",
    "generator",
    "React",
    "linked",
    "data",
    "RDF",
    "OWL"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au> (https://github.com/jeswr/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schimatos/shacl-form-react/issues"
  },
  "homepage": "https://github.com/schimatos/shacl-form-react#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@comunica/actor-init-sparql": "1.22",
    "@comunica/utils-datasource": "1.22",
    "@jeswr/react-lazy-render": "^1.0.1",
    "@ldfields/default-react": "file:../LDfields/packages/default-react",
    "@ldflex/comunica": "^3.5.0",
    "@ontologies/core": "^2.0.2",
    "@rdfjs/types": "^1.0.1",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/deindent": "^0.1.0",
    "@types/jest": "^27.4.0",
    "@types/jest-axe": "^3.5.3",
    "@types/rdf-js": "^4.0.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "buffer": "^6.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-extra-rules": "^0.0.0-development",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.4.5",
    "jest-axe": "^5.0.1",
    "jest-extended": "^1.2.0",
    "jsonld-streaming-parser": "^2.4.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "pre-commit": "^1.2.2",
    "rdf-object": "^1.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "semantic-release": "^18.0.1",
    "shacl-test-as-object": "^1.3.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "typedoc": "^0.22.10",
    "typescript": "4.3",
    "web-streams-polyfill": "^3.2.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**"
    ],
    "coveragePathIgnorePatterns": [
      "lib/types/*"
    ],
    "testRegex": "__tests__/[a-z-\\/]*-test.tsx?",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "dependencies": {
    "@jeswr/use-async-effect": "^1.0.0",
    "@jeswr/use-state": "^1.0.3",
    "@ontologies/rdf": "^2.0.0",
    "@ontologies/rdfs": "^2.0.1",
    "@rdfjs/data-model": "^1.3.4",
    "copy-anything": "^2.0.3",
    "deindent": "^0.1.0",
    "ldflex": "^2.13.0",
    "n3": "^1.12.2",
    "rdf-object-proxy": "^1.0.0",
    "rdf-string-ttl": "^1.2.0",
    "sparql-search-bar": "1.0.0-alpha.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
